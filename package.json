{
  "name": "workflow-ca",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "test": "npm run test-unit",
    "test-all": "npm run test-e2e",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run",
    "test-unit": "jest",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^10.11.0",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "^3.1.0",
    "sass": "^1.54.8",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3",
    "dotenv": "^16.3.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
