    "jest": "^29.2.0",
    "live-server": "^1.2.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "vite": "^3.2.2"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
