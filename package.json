{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "- Initialized npm",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "vite-build": "vite build",
    "vite-preview": "vite preview",
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "prepare": "husky install",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/_.js",
    "lint-fix": "eslint src/**/_.js --cache --fix",
    "test": "npm run test-unit",
    "test-unit": "jest --passWithNoTests"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "eslint": "^8.27.0",
    "eslint-plugin-jest": "^27.1.4",
    "husky": "^8.0.2",
    "jest": "^29.2.0",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steffenkjekken/social-media-client.git"
  },
  "bugs": {
    "url": "https://github.com/steffenkjekken/social-media-client/issues"
  },
  "homepage": "https://github.com/steffenkjekken/social-media-client#readme",
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
