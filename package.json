{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "format": "prettier -w src/**/*.js",
    "prepare": "husky install",
    "test": "npm run test-unit",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "cy-my-test": "cypress run --spec 'cypress/e2e/3-CA'"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.6.2",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-plugin-cypress": "^2.14.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.0",
    "live-server": "^1.2.2",
    "prettier": "^3.0.2",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
