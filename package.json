    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run"
  },
  "keywords": [],
  "author": "Peter",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.7.0",
    "eslint": "^8.32.0",
