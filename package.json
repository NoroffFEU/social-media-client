{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Git commit -m \"whatever\" makes eslint and prettier run tro alle my .js, .html files and fix them up",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "test": "jest sum.test.js",
    "dev": "vite",
    "preview": "vite preview",
    "build-vite": "vite build",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^11.0.0",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.1",
    "jest": "^29.3.0",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "sass": "^1.56.1",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run test"
    ],
    "*.{html,js}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sanhamm/social-media-client.git"
  },
  "bugs": {
    "url": "https://github.com/Sanhamm/social-media-client/issues"
  },
  "homepage": "https://github.com/Sanhamm/social-media-client#readme"
}
