{
  "name": "workflow",
  "version": "1.0.0",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "lint-staged": "^14.0.0",
    "live-server": "^1.1.0",
    "node-sass": "^9.0.0",
    "prettier": "^3.0.2",
    "sass": "^1.65.1"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-dark-5": "^1.1.3",
    "install": "^0.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErlendE96/social-media-client.git"
  },
  "bugs": {
    "url": "https://github.com/ErlendE96/social-media-client/issues"
  },
  "homepage": "https://github.com/ErlendE96/social-media-client#readme"
}
