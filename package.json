    "lint": "eslint src/**/_.js",
    "lint-fix": "eslint src/**/_.js --cache --fix"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steffenkjekken/social-media-client.git"
  },
  "bugs": {
    "url": "https://github.com/steffenkjekken/social-media-client/issues"
  },
  "homepage": "https://github.com/steffenkjekken/social-media-client#readme",
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
