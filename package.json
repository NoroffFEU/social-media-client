{
  "name": "css-frameworks-ca",
  "version": "1.0.3",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "test": "npm run test-unit && npm run test-e2e",
    "test-unit": "jest",
    "format": "prettier -w src/js/**/*.js & prettier -w src/js/*.js",
    "lint": "eslint src/js/**/*.js",
    "lint-fix": "eslint src/js/**/*.js --cache --fix",
    "prepare": "husky install",
    "test-e2e": "cypress open"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^10.7.0",
    "eslint": "^8.27.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.1.4",
    "husky": "^8.0.2",
    "jest": "^29.2.0",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bushrakalaji/Workflow-ca.git"
  },
  "bugs": {
    "url": "https://github.com/bushrakalaji/Workflow-ca/issues"
  },
  "homepage": "https://github.com/bushrakalaji/Workflow-ca#readme",
  "description": "",
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  }
}
