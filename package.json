{
  "name": "workflow-ca",
  "version": "1.0.4",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test-e2e && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run",
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "format": "prettier -w src/js/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^11.2.0",
    "eslint": "^8.27.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.1.3",
    "husky": "^8.0.2",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^27.0.6",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PJLUDVIGSEN/social-media-client-tester-PJ.git"
  },
  "bugs": {
    "url": "https://github.com/PJLUDVIGSEN/social-media-client-tester-PJ/issues"
  },
  "homepage": "https://github.com/PJLUDVIGSEN/social-media-client-tester-PJ#readme",
  "description": ""
}
