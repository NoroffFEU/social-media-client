    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "live-server": "^1.2.2",
    "prettier": "^2.8.4",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
