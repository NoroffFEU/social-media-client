{
  "name": "workflow-ca",
  "version": "0.0.1",
  "description": "CA workflow",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "format": "prettier -w src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Marthe B",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.27.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "webpack": "5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marthebull/workflow-CA-07-11-2022.git"
  },
  "bugs": {
    "url": "https://github.com/marthebull/workflow-CA-07-11-2022/issues"
  },
  "homepage": "https://github.com/marthebull/workflow-CA-07-11-2022#readme",
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.src": "prettier --write"
  }
}
