{
  "name": "css-frameworks-ca",
  "version": "0.0.3",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w index.js",
    "lint": "eslint src/js/**/*.js",
    "lint-fix": "eslint src/js/**/*.js --cache --fix",
    "prepare": "husky install",
    "test": "npm run test-unit",
    "test-unit": "jest"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "mjs",
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "\\.pnp\\.[^\\/]+$"
    ]
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "eslint": "^8.53.0",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.0",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "^3.0.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  }
}
