    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "live-server": "^1.2.2",
    "prettier": "^2.8.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
