{
    "env": {
        "browser": true,
        "es2021": true,
        "amd": true,
        "jest": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
        {
          "files": ["**/*.cy.js"],
          "env": { "jest": true },
          "plugins": ["jest"],
          "extends": ["plugin:jest/recommended"],
          "rules": { "jest/prefer-expect-assertions": "off" 
        }
        }
      ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn"

    }
}
